{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "env": {
        "browser": true
    },
    "rules": {
        "indent": [
            "error",
            4
        ],
        "react/jsx-indent": [
            "error",
            4
        ],
        "react/jsx-indent-props": [
            "error",
            4
        ],
        "import/prefer-default-export": 0,
        "import/no-extraneous-dependencies": 0,
        "react/jsx-filename-extension": 0,
        "react/forbid-prop-types": 0,
        "react/require-default-props": 0,
        "react/no-unused-prop-types": 0
    },
    "globals": {
        "jest": true,
        "describe": true,
        "beforeEach": true,
        "it": true,
        "expect": true,
        "afterEach": true,
    }
}
